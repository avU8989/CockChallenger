<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>com.example</groupId> <!-- Replace with your package name -->
    <artifactId>cock-challenger</artifactId> <!-- Replace with your project name -->
    <version>1.0.0</version>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.8.1</version>
                <configuration>
                    <source>17</source> <!-- Set this to your desired Java version -->
                    <target>17</target> <!-- Match the source version -->
                </configuration>
            </plugin>
        </plugins>
    </build>


    <dependencies>
        <!-- Add your dependencies here -->
    </dependencies>
</project>
